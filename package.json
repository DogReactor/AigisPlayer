    "@types/jasmine": "^3.3.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.10",
    "@types/request": "^2.48.1",
    "@types/rx": "^4.1.1",
    "@types/unzip": "^0.1.1",
    "autoprefixer": "9.3.1",
    "circular-dependency-plugin": "5.0.2",
    "codelyzer": "4.5.0",
    "copy-webpack-plugin": "4.6.0",
    "copyfiles": "2.1.0",
    "core-js": "2.5.7",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
